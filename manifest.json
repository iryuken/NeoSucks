{
   "manifest_version": 3,
   "name": "NeoExamShield",
   "description": "Bypass",
   "key": "",
   "host_permissions": [
      "https://*/*"
   ],
   "version": "2.6.6",
   "version_name": "Varience - v2.6.6",
   "action": {
      "default_icon": {
         "128": "images/icon128.png",
         "16": "images/icon16.png",
         "48": "images/icon48.png"
      },
      "default_popup": ""
   },
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "js": [
            "content-script.js",
            "interceptor.js",
            "hooks/ace-mod.js",
            "hooks/bypass.js",
            "hooks/nyxie.js"
         ],
         "matches": [
            "https://*.examly.io/*",
            "https://placements.mietjmu.in/*",
            "https://placement.skcet.ac.in/*"
         ],
         "run_at": "document_end",
         "world": "MAIN",
         "exclude_matches": [
            "https://*.examly.io/result*",
            "https://*.examly.io/login",
            "https://*.examly.io/pwd"
         ],
         "matchOriginAsFallback": true
      }
   ],
   "permissions": [
      "management",
      "tabs",
      "storage"
   ],
   "externally_connectable": {
      "matches": [
         "*://*.examly.io/*",
         "https://placements.mietjmu.in/*"
      ]
   }
}
